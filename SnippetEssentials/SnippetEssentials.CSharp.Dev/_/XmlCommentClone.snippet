<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Xml Comment for Clone</Title>
            <Shortcut>xcclone</Shortcut>
            <Description>Xml comment for clone method.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Declarations>
                <Literal Editable="false">
                    <ID>thisname</ID>
                    <ToolTip>Containing type name</ToolTip>
                    <Default>ThisName</Default>
                    <Function>ClassName()</Function>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[/// <summary>
/// Creates a new <see cref="$thisname$"/> that is a copy of the current instance.
/// </summary>
/// <returns>A new <see cref="$thisname$"/> that is a copy of the current instance.</returns>$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
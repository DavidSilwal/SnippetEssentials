<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Private Read-only Field DateTime (With Initialization)</Title>
            <Shortcut>prfdt_</Shortcut>
            <Description>Private read-only DateTime field (with initialization).</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Imports>
                <Import>
                    <Namespace>System</Namespace>
                </Import>
            </Imports>
            <Declarations>
                <Literal>
                    <ID>name</ID>
                    <ToolTip>Field name</ToolTip>
                    <Default>fieldName</Default>
                </Literal>
                <Literal>
                    <ID>value</ID>
                    <ToolTip>Initial value of the field</ToolTip>
                    <Default>DateTime.Now</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[private readonly DateTime $name$ = $value$;$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
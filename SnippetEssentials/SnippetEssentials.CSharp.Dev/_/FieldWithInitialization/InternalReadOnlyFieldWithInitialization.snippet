<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Internal Read-only Field (With Initialization)</Title>
            <Shortcut>irf_</Shortcut>
            <Description>Internal read-only field (with initialization).</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Declarations>
                <Literal>
                    <ID>_fieldType</ID>
                    <ToolTip>Field type</ToolTip>
                    <Default>string</Default>
                </Literal>
                <Literal>
                    <ID>name</ID>
                    <ToolTip>Field name</ToolTip>
                    <Default>FieldName</Default>
                </Literal>
                <Literal>
                    <ID>value</ID>
                    <ToolTip>Initial value of the field</ToolTip>
                    <Default>null</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[internal readonly $_fieldType$ $name$ = $value$;$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
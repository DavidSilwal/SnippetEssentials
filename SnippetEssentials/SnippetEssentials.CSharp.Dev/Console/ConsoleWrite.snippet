<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Console Write</Title>
            <Shortcut>cew</Shortcut>
            <Description>Console.Write method.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Imports>
                <Import>
                    <Namespace>System</Namespace>
                </Import>
            </Imports>
            <Declarations>
                <Literal>
                    <ID>args</ID>
                    <ToolTip>Method arguments</ToolTip>
                    <Default>args</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[Console.Read($args$);$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
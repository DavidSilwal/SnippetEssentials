<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Variable Stopwatch</Title>
            <Shortcut>vstopwatch</Shortcut>
            <Description>Stopwatch variable declaration.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Imports>
                <Import>
                    <Namespace>System</Namespace>
                </Import>
                <Import>
                    <Namespace>System.Diagnostics</Namespace>
                </Import>
                <Import>
                    <Namespace>System.Diagnostics</Namespace>
                </Import>
            </Imports>
            <Declarations>
                <Literal>
                    <ID>name</ID>
                    <ToolTip>Variable name</ToolTip>
                    <Default>sw</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[Stopwatch $name$ = Stopwatch.StartNew();

$end$

$name$.Stop();
Console.WriteLine($name$.Elapsed);
Debug.WriteLine($name$.Elapsed);]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
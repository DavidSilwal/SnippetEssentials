<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Variable ImmutableArray&lt;T&gt; using Create Method</Title>
            <Shortcut>viea</Shortcut>
            <Description>ImmutableArray&lt;T&gt; variable declaration using Create method.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
            <Keywords>
                <Keyword>NonUniqueShortcut</Keyword>
            </Keywords>
        </Header>
        <Snippet>
            <Imports>
                <Import>
                    <Namespace>System.Collections.Immutable</Namespace>
                </Import>
            </Imports>
            <Declarations>
                <Literal>
                    <ID>_vararray</ID>
                    <ToolTip>Array variable name</ToolTip>
                    <Default>items</Default>
                </Literal>
                <Literal>
                    <ID>arguments</ID>
                    <ToolTip>Method arguments</ToolTip>
                    <Default>arguments</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[var $_vararray$ = ImmutableArray.Create($arguments$);$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
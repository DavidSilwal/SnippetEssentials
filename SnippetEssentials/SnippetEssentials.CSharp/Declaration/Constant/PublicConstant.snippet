<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Public Constant</Title>
            <Shortcut>k</Shortcut>
            <Description>Public constant.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
            <Keywords>
                <Keyword>CoreSnippet</Keyword>
            </Keywords>
        </Header>
        <Snippet>
            <Declarations>
                <Literal>
                    <ID>_consttype</ID>
                    <ToolTip>Constant type</ToolTip>
                    <Default>string</Default>
                </Literal>
                <Literal>
                    <ID>name</ID>
                    <ToolTip>Constant name</ToolTip>
                    <Default>ConstantName</Default>
                </Literal>
                <Literal>
                    <ID>value</ID>
                    <ToolTip>Constant value</ToolTip>
                    <Default>""</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[public const $_consttype$ $name$ = $value$;$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
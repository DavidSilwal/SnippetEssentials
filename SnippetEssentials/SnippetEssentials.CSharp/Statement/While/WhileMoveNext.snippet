<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>While MoveNext</Title>
            <Shortcut>wemn</Shortcut>
            <Description>While statement with condition whether enumerator MoveNext() method returns true.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Declarations>
                <Literal>
                    <ID>enumerator</ID>
                    <ToolTip>Enumerator variable name</ToolTip>
                    <Default>en</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[while ($enumerator$.MoveNext())
{
    $end$
}]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
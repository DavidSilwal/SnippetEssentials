<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>#Warning Preprocessor Directive</Title>
            <Shortcut>ppw</Shortcut>
            <Description>#warning preprocessor directive.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Declarations>
                <Literal>
                    <ID>warning</ID>
                    <ToolTip>Warning text</ToolTip>
                    <Default>warning</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[#warning $warning$$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
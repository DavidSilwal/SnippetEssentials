<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Object Equals</Title>
            <Shortcut>oq</Shortcut>
            <Description>Object.Equals method.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
            <Keywords>
                <Keyword>Meta-CoreSnippet</Keyword>
            </Keywords>
        </Header>
        <Snippet>
            <Imports>
                <Import>
                    <Namespace>System</Namespace>
                </Import>
            </Imports>
            <Declarations>
                <Literal Editable="false">
                    <ID>_object</ID>
                    <Default>object</Default>
                </Literal>
                <Literal>
                    <ID>first</ID>
                    <ToolTip>First variable name</ToolTip>
                    <Default>x</Default>
                </Literal>
                <Literal>
                    <ID>second</ID>
                    <ToolTip>Second variable name</ToolTip>
                    <Default>y</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[$_object$.Equals($first$, $second$)$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Variable List&lt;T&gt;</Title>
            <Shortcut>vl</Shortcut>
            <Description>List&lt;T&gt; variable declaration.</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
            <Keywords>
                <Keyword>CoreSnippet</Keyword>
            </Keywords>
        </Header>
        <Snippet>
            <Imports>
                <Import>
                    <Namespace>System.Collections.Generic</Namespace>
                </Import>
            </Imports>
            <Declarations>
                <Literal>
                    <ID>_generictype</ID>
                    <ToolTip>Generic type</ToolTip>
                    <Default>object</Default>
                </Literal>
                <Literal>
                    <ID>_varlist</ID>
                    <ToolTip>List variable name</ToolTip>
                    <Default>items</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[var $_varlist$ = new List<$_generictype$>();$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>Variable ObservableCollection&lt;T&gt; with Argument</Title>
            <Shortcut>voc_</Shortcut>
            <Description>ObservableCollection&lt;T&gt; variable declaration (with argument).</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Imports>
                <Import>
                    <Namespace>System.Collections.ObjectModel</Namespace>
                </Import>
            </Imports>
            <Declarations>
                <Literal>
                    <ID>_generictype</ID>
                    <ToolTip>Generic type</ToolTip>
                    <Default>object</Default>
                </Literal>
                <Literal>
                    <ID>_varcollection</ID>
                    <ToolTip>Collection variable name</ToolTip>
                    <Default>items</Default>
                </Literal>
                <Literal>
                    <ID>argument</ID>
                    <ToolTip>Constructor argument</ToolTip>
                    <Default>collection</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[var $_varcollection$ = new ObservableCollection<$_generictype$>($argument$);$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>
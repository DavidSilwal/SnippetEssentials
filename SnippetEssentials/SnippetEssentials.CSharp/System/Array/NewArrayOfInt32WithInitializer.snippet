<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
    <CodeSnippet Format="1.0.0">
        <Header>
            <Title>New Array of Int32 with Initializer</Title>
            <Shortcut>naiir</Shortcut>
            <Description>New array of Int32 (with initializer).</Description>
            <Author>Josef Pihrt</Author>
            <SnippetTypes>
                <SnippetType>Expansion</SnippetType>
            </SnippetTypes>
        </Header>
        <Snippet>
            <Declarations>
                <Literal>
                    <ID>init</ID>
                    <ToolTip>Array elements initialization</ToolTip>
                    <Default>init</Default>
                </Literal>
            </Declarations>
            <Code Language="CSharp"><![CDATA[new int[] { $init$ }$end$]]></Code>
        </Snippet>
    </CodeSnippet>
</CodeSnippets>